/* 轮播图 */
.swiper_index swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper_index swiper image {
  width: 100%;
}
/* 导航 */
.cate_index {
  display: flex;
}
.cate_index navigator {
  flex: 1;
  padding: 20rpx;
}
.cate_index navigator image {
  width: 100%;
}
/* 楼层 */
.floor_index .floor_group .floor_title {
  padding: 10rpx 0;
}
.floor_index .floor_group .floor_title image {
  width: 100%;
}
.floor_index .floor_group .floor_list {
  overflow: hidden;
}
.floor_index .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
}
.floor_index .floor_group .floor_list navigator:nth-last-child(-n+4) {
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.floor_index .floor_group .floor_list navigator:nth-child(2),
.floor_index .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_index .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
